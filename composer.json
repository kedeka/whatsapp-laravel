{
    "name": "kedeka/whatsapp-laravel",
    "description": "Whatsapp Laravel Package for Kedeka.com",
    "keywords": [
        "kedeka",
        "laravel",
        "whatsapp-laravel"
    ],
    "homepage": "https://github.com/kedeka/whatsapp-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Rizky Hajar",
            "email": "riskihajar@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/contracts": "^7.0|^8.0|^9.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "nunomaduro/collision": "^4.0|^5.0|^6.0",
        "orchestra/testbench": "^5.0|^6.0|^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Kedeka\\Whatsapp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kedeka\\Whatsapp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kedeka\\Whatsapp\\ServiceProvider"
            ],
            "aliases": {
                "Whatsapp": "Kedeka\\Whatsapp\\Facades\\WhatsApp"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
